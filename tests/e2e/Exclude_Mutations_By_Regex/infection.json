{
    "timeout": 25,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "summary": "infection.log"
    },
    "tmpDir": ".",
    "mutators": {
        "PublicVisibility": {
            "ignore": [
                "App\\*"
            ],
            "ignoreSourceCodeByRegex": [
                "getString"
            ]
        },
        "MethodCallRemoval": {
            "ignoreSourceCodeByRegex": [
                "Assert::",
                "\\$this->getString\\(\\)"
            ]
        }
    }
}
